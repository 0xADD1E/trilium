{
  "name": "trilium",
  "productName": "Trilium Notes",
  "description": "Trilium Notes",
  "version": "0.40.4",
  "license": "AGPL-3.0-only",
  "main": "electron.js",
  "bin": {
    "trilium": "./src/www"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zadam/trilium.git"
  },
  "scripts": {
    "start-server": "TRILIUM_ENV=dev node ./src/www",
    "start-electron": "TRILIUM_ENV=dev electron .",
    "build-backend-docs": "./node_modules/.bin/jsdoc -c jsdoc-conf.json -d ./docs/backend_api src/entities/*.js src/services/backend_script_api.js",
    "build-frontend-docs": "./node_modules/.bin/jsdoc -c jsdoc-conf.json -d ./docs/frontend_api src/public/javascripts/entities/*.js src/public/javascripts/services/frontend_script_api.js",
    "build-docs": "npm run build-backend-docs && npm run build-frontend-docs"
  },
  "dependencies": {
    "async-mutex": "0.1.4",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "cls-hooked": "4.2.2",
    "commonmark": "0.29.1",
    "cookie-parser": "1.4.4",
    "csurf": "1.11.0",
    "dayjs": "1.8.21",
    "debug": "4.1.1",
    "ejs": "2.7.4",
    "electron-debug": "3.0.1",
    "electron-dl": "3.0.0",
    "electron-find": "1.0.6",
    "electron-window-state": "5.0.3",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "file-type": "14.1.3",
    "fs-extra": "8.1.0",
    "helmet": "3.21.3",
    "html": "1.0.0",
    "html2plaintext": "2.1.2",
    "http-proxy-agent": "4.0.1",
    "https-proxy-agent": "5.0.0",
    "image-type": "4.1.0",
    "imagemin": "7.0.1",
    "imagemin-giflossy": "5.1.10",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "8.0.0",
    "ini": "1.3.5",
    "jimp": "0.9.5",
    "mime-types": "2.1.26",
    "multer": "1.4.2",
    "node-abi": "2.15.0",
    "open": "7.0.2",
    "portscanner": "2.2.0",
    "rand-token": "0.4.0",
    "rcedit": "2.1.0",
    "rimraf": "3.0.2",
    "sanitize-filename": "1.6.3",
    "sax": "1.2.4",
    "semver": "7.1.3",
    "serve-favicon": "2.5.0",
    "session-file-store": "1.4.0",
    "simple-node-logger": "18.12.24",
    "sqlite": "3.0.3",
    "sqlite3": "4.1.1",
    "string-similarity": "4.0.1",
    "tar-stream": "2.1.1",
    "turndown": "5.0.3",
    "turndown-plugin-gfm": "1.0.2",
    "unescape": "1.0.1",
    "ws": "7.2.1"
  },
  "devDependencies": {
    "electron": "9.0.0-beta.7",
    "electron-builder": "22.4.0",
    "electron-packager": "14.2.1",
    "electron-rebuild": "1.10.0",
    "jsdoc": "3.6.3",
    "lorem-ipsum": "2.0.3"
  },
  "optionalDependencies": {
    "electron-installer-debian": "2.0.1"
  }
}
